{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "events",
      "direction": "in",
      "connection": "AZURE_EVENT_HUB_CONN_STR",
      "eventHubName": "%AZURE_EVENT_HUB_NAME%",
      "cardinality": "many",
      "consumerGroup": "consumer1"
    },
    {
      "type": "eventHub",
      "name": "outputEvent",
      "direction": "out",
      "connection": "OUT_AZURE_EVENT_HUB_CONN_STR",
      "eventHubName": "%OUT_AZURE_EVENT_HUB_NAME%",
      "cardinality": "one"
    }
  ]
}